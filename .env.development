# PORT
APP_NAME = ExpressJS
PORT = 5000
NODE_ENV=development

# DATABASE
DB_HOST = localhost
DB_PORT = 27017
DB_DATABASE = dev

# TOKEN
SECRET_KEY = secretKey

# LOG
LOG_FORMAT = dev
LOG_DIR = '../../logs'
LOG_STORE = true
LOG_MAX_FILES = 30

# CORS
ORIGIN = *
CREDENTIALS = true

# Nginx
NGINX_SERVER_NAME=localhost
NGINX_SERVER_HOST=localhost

NO_COLOR=false

LOGS_DIR='./logs'
